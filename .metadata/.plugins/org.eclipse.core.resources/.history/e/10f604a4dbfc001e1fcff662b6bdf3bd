package com.nit.datatypes;

public class TestDatatype {

	public static void main(String[] args) {
		
		byte a = 10;
		short b = 20;
		int c = 30;
		long d = 40;
		float e = 50;
		double f = 60;
		char g = 70;
		
		m1(a);
		
	}
	
	public static void m1(byte a) {
		System.out.println("m1 executed");
	}
	
}
